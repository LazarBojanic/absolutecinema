setup Shirt{
	var color: string;
	var size: int;
	Shirt(var color: string, var size: int){
		@.color = color;
		@.size = size;
	}
	scene getColor() : string{
		cut @.color;
	}
}
scene entrance(var args: string[]): scrap {
	var shirt: Shirt = action Shirt("red", 32);
	project(shirt.getColor());
	if(shirt.size > 30){
		project("large");
	}
	elif(shirt.size > 20 && shirt.size <= 30){
		project("medium");
	}
	else{
		project("small");
	}
}