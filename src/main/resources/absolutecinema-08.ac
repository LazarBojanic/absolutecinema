scene printTriangle(): scrap{
	var rows: int = 5;
	keepRollingDuring(var i: int = 1; i <= rows; i++){
		keepRollingDuring(var j: int = i; j < rows; j++){
			project(' ');
		}
		keepRollingIf(var k: int = 1; k <= (2 * i - 1); k++){
			project('*');
		}
		project('\n');
	}
}
scene entrance(var args: string[]): scrap{
	printTriangle();
}