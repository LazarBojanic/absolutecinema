scene binarySearch(var arr: int[], var len: int, var x: int): int{
	var left: int = 0;
	var right: int = len - 1;
	keepRollingIf(left <= right){
		var mid: int = left + (right - left) / 2;
		if(arr[mid] == x){
			cut mid;
		}
		elif(arr[mid] < x) {
			left = mid + 1;
		}
		else {
			right = mid - 1;
		}
	}
	cut -1;
}

scene entrance(var args: string[]): scrap{
	var arr: int[] = action int[10] {2, 4, 5, 7, 16, 25, 33, 39, 49, 69};
	var x: int = 5;
	binarySearch(arr, 10, x);
}